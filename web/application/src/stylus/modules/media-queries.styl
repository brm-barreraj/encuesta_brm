  .item
    &-logo
      &.active
        .container-info-cliente
          max-width 70%


@media (max-width:630px)

  .bienestar
    &-content
      h2
        font-size 3rem
        max-width 100%

  .container
    &-banner-home
      margin-bottom vr-block(-1)

    &-columnas
      .col-xs-4
        clear both
        float none
        margin vr-block(0.5) auto
        width 100%
    &-social-icon
      bottom -4rem

      a
        padding 0.3rem

    &-title
      font-size 3rem
    &-content
      margin-top 15rem
    &-image-hero
      h1
        bottom -5rem
        font-size 2.4rem
        max-width 100%
        width 100%
    &-servicios
      .col-lg-3
        width 50%
  .interacciones
    &-circle
      display none

  .item
    &-logo
      &.active
        padding-bottom 100%

        .container-info-cliente
          max-width 100%
          p
            font-size 1.4rem

          .container-social-icon
            max-width 12%
            position absolute
            right 0
            top 4rem

  .slide
    &-content
      margin-top vr-block(0.5)

    &-image
      height 37rem
      img
        height 40rem

    &-title
      font-size 1.4rem
      margin-top vr-block(6.5)

  .slider
    &-home
      margin-bottom 1rem

